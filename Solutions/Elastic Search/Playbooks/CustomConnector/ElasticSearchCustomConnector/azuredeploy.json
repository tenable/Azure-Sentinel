{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "CustomConnectorName": {
            "defaultValue": "ElasticSearchCustomConnector",
            "type": "String",
            "metadata": {
                "description": "Name of the custom connector, if you want to change the default name, make sure to use the same in all Elastic Search automation playbooks as well"
            }
        },
        "ElasticSearchHostURL": {
            "defaultValue": "ElasticServerNameORIPAddress:9200",
            "type": "String",
            "metadata": {
                "description": "Enter your Elastic Search url for example: ElasticServerNameORIPAddress:9200. Do NOT prefix with https:// or http:// etc"
            }
        }
    },
    "variables": {
        "api_host": "[replace(replace(parameters('ElasticSearchHostURL'),'https://',''),'http://','')]",
        "ServiceName": "[concat('https://', variables('api_host'))]"
    },
    "resources": [
        {
            "type": "Microsoft.Web/customApis",
            "apiVersion": "2016-06-01",
            "name": "[parameters('CustomConnectorName')]",
            "location": "[resourceGroup().location]",
            "properties": {
                "connectionParameters": {
                    "api_key": {
                        "type": "securestring",
                        "uiDefinition": {
                            "displayName": "API Key",
                            "description": "The API Key for Elastic Search api",
                            "tooltip": "Provide your API Key starts with ApiKey <<Key Value>>",
                            "constraints": {
                                "tabIndex": 2,
                                "clearText": false,
                                "required": "true"
                            }
                        }
                    }
                },
                "brandColor": "#FFFFFF",
                "description": "[concat('This connector helps to authenticate with ', parameters('CustomConnectorName'), ' elastic search andd running elastic search queries')]",
                "displayName": "[parameters('CustomConnectorName')]",
                "iconUri": "data:image/png;base64,",
                "backendService": {
                    "serviceUrl": "[variables('ServiceName')]"
                },
                "apiType": "Rest",
                "swagger": {
                    "swagger": "2.0",
                    "info": {
                        "version": "1.0.0",
                        "title": "ElasticSearch",
                        "description": "Elastic Search connector that helps to search and retrieve data from elastic platform"
                    },
                    "host": "[replace(replace(parameters('ElasticSearchHostURL'),'https://',''),'http://','')]",
                    "basePath": "/",
                    "schemes": [ "https" ],
                    "consumes": [],
                    "produces": [ "application/json" ],
                    "paths": {
                        "/%7Bindex%7D/_search": {},
                        "/%7Bindex%7D/_async_search": {},
                        "/_async_search/status/%7BSearchId%7D": {},
                        "/_async_search/%7BSearchId%7D": {},
                        "/%7Bindex%7D/_search_shards": {},
                        "/{index}/_search": {
                            "post": {
                                "summary": "Run Search",
                                "description": "Run Search",
                                "operationId": "RunSearch",
                                "parameters": [
                                    {
                                        "name": "index",
                                        "in": "path",
                                        "required": true,
                                        "type": "string"
                                    },
                                    {
                                        "name": "size",
                                        "in": "query",
                                        "required": false,
                                        "type": "integer"
                                    },
                                    {
                                        "name": "sort",
                                        "in": "query",
                                        "required": false,
                                        "type": "string"
                                    },
                                    {
                                        "name": "from",
                                        "in": "query",
                                        "required": false,
                                        "type": "integer"
                                    },
                                    {
                                        "name": "Content-Type",
                                        "in": "header",
                                        "required": true,
                                        "type": "string",
                                        "default": "application/json",
                                        "description": "Content-Type",
                                        "x-ms-visibility": "internal"
                                    },
                                    {
                                        "name": "ElasticQuery",
                                        "in": "body",
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "query": {
                                                    "type": "object",
                                                    "description": "Type your elastic query here",
                                                    "title": "ElasticQuery",
                                                    "x-ms-visibility": "important",
                                                    "default": {}
                                                }
                                            }
                                        },
                                        "x-ms-visibility": "important"
                                    }
                                ],
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": {}
                                    }
                                }
                            }
                        },
                        "/{index}/_async_search": {
                            "post": {
                                "summary": "Run Async-Search",
                                "description": "Run Async-Search",
                                "operationId": "RunAsync-search",
                                "parameters": [
                                    {
                                        "name": "index",
                                        "in": "path",
                                        "required": true,
                                        "type": "string"
                                    },
                                    {
                                        "name": "size",
                                        "in": "query",
                                        "required": false,
                                        "type": "integer"
                                    },
                                    {
                                        "name": "sort",
                                        "in": "query",
                                        "required": false,
                                        "type": "string"
                                    },
                                    {
                                        "name": "from",
                                        "in": "query",
                                        "required": false,
                                        "type": "integer"
                                    },
                                    {
                                        "name": "Content-Type",
                                        "in": "header",
                                        "required": true,
                                        "type": "string",
                                        "default": "application/json",
                                        "description": "Content-Type",
                                        "x-ms-visibility": "internal"
                                    },
                                    {
                                        "name": "Elastic Query",
                                        "in": "body",
                                        "required": true,
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "query": {
                                                    "type": "object",
                                                    "description": "Type your elastic query here",
                                                    "title": "ElasticQuery",
                                                    "x-ms-visibility": "important",
                                                    "default": {}
                                                }
                                            },
                                            "x-ms-visibility": "important"
                                        },
                                        "x-ms-visibility": "important"
                                    }
                                ],
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": {}
                                    }
                                }
                            }
                        },
                        "/_async_search/status/{SearchId}": {
                            "get": {
                                "summary": "Get Async-Search Status",
                                "description": "Get Async-Search Status",
                                "operationId": "GetAsync-searchStatus",
                                "parameters": [
                                    {
                                        "name": "SearchId",
                                        "in": "path",
                                        "required": true,
                                        "type": "string"
                                    },
                                    {
                                        "name": "Content-Type",
                                        "in": "header",
                                        "required": true,
                                        "type": "string",
                                        "default": "application/json",
                                        "description": "Content-Type",
                                        "x-ms-visibility": "internal"
                                    }
                                ],
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": {}
                                    }
                                }
                            }
                        },
                        "/_async_search/{SearchId}": {
                            "post": {
                                "summary": "Get Async-Search Result",
                                "description": "Get Async-Search Result",
                                "operationId": "GetAsync-searchResult",
                                "parameters": [
                                    {
                                        "name": "SearchId",
                                        "in": "path",
                                        "required": true,
                                        "type": "string"
                                    },
                                    {
                                        "name": "Content-Type",
                                        "in": "header",
                                        "required": true,
                                        "type": "string",
                                        "default": "application/json",
                                        "description": "Content-Type",
                                        "x-ms-visibility": "internal"
                                    }
                                ],
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": {}
                                    }
                                }
                            }
                        },
                        "/{index}/_search_shards": {
                            "get": {
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": { "type": "string" }
                                    }
                                },
                                "summary": "Search Shards",
                                "operationId": "SearchShards",
                                "description": "Returns the indices and shards that a search request would be executed against.",
                                "parameters": [
                                    {
                                        "name": "Content-Type",
                                        "in": "header",
                                        "required": true,
                                        "type": "string",
                                        "default": "application/json",
                                        "x-ms-visibility": "internal"
                                    },
                                    {
                                        "name": "index",
                                        "in": "path",
                                        "required": true,
                                        "type": "string"
                                    }
                                ]
                            }
                        }
                    },
                    "definitions": {},
                    "parameters": {},
                    "responses": {},
                    "securityDefinitions": {
                        "API Key": {
                            "type": "apiKey",
                            "in": "header",
                            "name": "Authorization"
                        }
                    },
                    "security": [ { "API Key": [] } ],
                    "tags": []
                }
            }
        }
    ]
}
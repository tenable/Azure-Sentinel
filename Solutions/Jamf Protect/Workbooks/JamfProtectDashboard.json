    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "jamfprotect_CL\n//| where header_event_name_s !contains \"AUE\"\n| where input_eventType_s <> \"\"\n|extend name_ = tostring(parse_json(input_match_facts_s)[0].name)\n|summarize count() by input_eventType_s, name_\n|project-rename EventType = input_eventType_s, Event = name_, Count = count_\n|sort by Count desc\n|limit 10",

    {
      "type": 12,
      "content": {
        "version": "NotebookGroup/1.0",
        "groupType": "editable",
        "title": "Threat Hunting {_timetoken:value}",
        "items": [
          {
            "type": 1,
            "content": {
              "json": "Set an type and provide values to search on File hash, CVE numbers or report on Alerts mapped to the MITRE framework.",
              "style": "info"
            },
            "name": "text - 2",
            "styleSettings": {
              "showBorder": true
            }
          },
          {
            "type": 9,
            "content": {
              "version": "KqlParameterItem/1.0",
              "parameters": [
                {
                  "id": "5f5886d0-e83e-4ffc-a48c-bfed7370aa66",
                  "version": "KqlParameterItem/1.0",
                  "name": "_type",
                  "label": "Type",
                  "type": 2,
                  "description": "Please choose the type",
                  "isGlobal": true,
                  "typeSettings": {
                    "additionalResourceOptions": [],
                    "showDefault": false
                  },
                  "jsonData": "[\n    { \"value\":\"filehash\", \"label\":\"File Hash\" },\n    { \"value\":\"CVE\", \"label\":\"CVE\" },\n    { \"value\":\"mitre\", \"label\":\"Framework: MITRE\" }\n]",
                  "value": null
                }
              ],
              "style": "pills",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces"
            },
            "customWidth": "50",
            "name": "Threat Hunting"
          },
          {
            "type": 9,
            "content": {
              "version": "KqlParameterItem/1.0",
              "parameters": [
                {
                  "id": "bcdd945e-4dfe-47d6-9489-f76ce012c224",
                  "version": "KqlParameterItem/1.0",
                  "name": "_filehash",
                  "label": "File Hash",
                  "type": 1,
                  "description": "Thish value can be used for searching all alerts for a certain hash",
                  "isRequired": true,
                  "isGlobal": true,
                  "value": ""
                }
              ],
              "style": "formHorizontal",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces"
            },
            "customWidth": "50",
            "conditionalVisibility": {
              "parameterName": "_type",
              "comparison": "isEqualTo",
              "value": "filehash"
            },
            "name": "Hash Search"
          },
          {
            "type": 9,
            "content": {
              "version": "KqlParameterItem/1.0",
              "parameters": [
                {
                  "id": "0344768b-16c4-44ec-a4ac-73a8bc83d0e2",
                  "version": "KqlParameterItem/1.0",
                  "name": "_CVE",
                  "label": "CVE Number",
                  "type": 1,
                  "description": "Please search on the CVE number",
                  "isRequired": true,
                  "isGlobal": true,
                  "value": ""
                }
              ],
              "style": "formHorizontal",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces"
            },
            "customWidth": "50",
            "conditionalVisibility": {
              "parameterName": "_type",
              "comparison": "isEqualTo",
              "value": "CVE"
            },
            "name": "CVE_values"
          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "jamfprotect_CL\n| where input_eventType_s == \"GPThreatMatchExecEvent\"\n| where input_related_binaries_s contains \"{_filehash:value}\"\n| extend Severity = case(input_match_severity_d == 0, \"Informational\", input_match_severity_d == 1, \"Low\", input_match_severity_d == 2, \"Medium\", input_match_severity_d == 3, \"High\", \"True\")\n| extend AlertURL = strcat(\"https://\", org_hd_s, \".jamfcloud.com/Alerts/\", input_match_uuid_g)\n| project-keep TimeGenerated, input_match_event_matchValue_s, Severity, input_match_event_matchType_s, input_match_event_process_args_s, input_host_hostname_s, AlertURL\n| project-rename Related_To = input_match_event_matchValue_s, EventType = input_match_event_matchType_s, Alert_Severity = Severity, Process_Arguments = input_match_event_process_args_s, Hostname = input_host_hostname_s\n| project-reorder TimeGenerated, Alert_Severity, EventType, Related_To, Process_Arguments, Alert_Severity, Hostname, AlertURL\n| sort by TimeGenerated",
              "size": 4,
              "title": "Matches on FileHash",
              "noDataMessage": "No matches found based on the hash value",
              "timeContextFromParameter": "_timetoken",
              "showExportToExcel": true,
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces"
            },
            "conditionalVisibilities": [
              {
                "parameterName": "_filehash",
                "comparison": "isNotEqualTo",
                "value": "null"
              },
              {
                "parameterName": "_type",
                "comparison": "isEqualTo",
                "value": "filehash"
              }
            ],
            "name": "query - 1"
          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "jamfprotect_CL\n| where parse_json(input_match_tags_s) contains \"{_CVE:value}\"\n| extend Severity = case(input_match_severity_d == 0, \"Informational\", input_match_severity_d == 1, \"Low\", input_match_severity_d == 2, \"Medium\", input_match_severity_d == 3, \"High\", \"True\")\n| project-keep TimeGenerated, input_match_event_matchValue_s, input_match_tags_s, Severity, input_match_event_matchType_s, input_match_event_process_args_s, input_host_hostname_s\n| project-rename Related_To = input_match_event_matchValue_s, CVE = input_match_tags_s, EventType = input_match_event_matchType_s, Alert_Severity = Severity, Process_Arguments = input_match_event_process_args_s, Hostname = input_host_hostname_s\n| sort by TimeGenerated",
              "size": 4,
              "title": "Matches on CVE",
              "noDataMessage": "No matches found based on the _CVE:value CVE number",
              "timeContextFromParameter": "_timetoken",
              "showExportToExcel": true,
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces"
            },
            "conditionalVisibilities": [
              {
                "parameterName": "_cve",
                "comparison": "isNotEqualTo",
                "value": "null"
              },
              {
                "parameterName": "_type",
                "comparison": "isEqualTo",
                "value": "CVE"
              }
            ],
            "name": "CVE Searcher"
          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "jamfprotect_CL\n| where parse_json(input_match_tags_s) contains \"MITREattack\"\n| extend Severity = case(input_match_severity_d == 0, \"Informational\", input_match_severity_d == 1, \"Low\", input_match_severity_d == 2, \"Medium\", input_match_severity_d == 3, \"High\", \"True\")\n| extend AlertURL = strcat(\"https://\", org_hd_s, \".jamfcloud.com/Alerts/\", input_match_uuid_g)\n| extend Analytic = parse_json(input_match_facts_s)[0].name\n| extend Description = parse_json(input_match_facts_s)[0].human\n| project-keep TimeGenerated, input_eventType_s, Analytic, Description, input_match_tags_s, Severity, input_host_hostname_s, AlertURL\n| project-rename EventType = input_eventType_s, TAGS = input_match_tags_s, Alert_Severity = Severity, Hostname = input_host_hostname_s\n| project-reorder TimeGenerated, EventType, Analytic, Description, TAGS, Alert_Severity, Hostname, AlertURL\n| sort by TimeGenerated",
              "size": 4,
              "title": "Alerts mapped to MITRE",
              "noDataMessage": "No alerts found that are mapped to the MITRE framework",
              "timeContextFromParameter": "_timetoken",
              "showExportToExcel": true,
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "gridSettings": {
                "sortBy": [
                  {
                    "itemKey": "TimeGenerated",
                    "sortOrder": 1
                  }
                ]
              },
              "sortBy": [
                {
                  "itemKey": "TimeGenerated",
                  "sortOrder": 1
                }
              ]
            },
            "conditionalVisibilities": [
              {
                "parameterName": "_type",
                "comparison": "isNotEqualTo",
                "value": "null"
              },
              {
                "parameterName": "_type",
                "comparison": "isEqualTo",
                "value": "mitre"
              }
            ],
            "name": "MITRE Searcher"
          }
        ]
      },
      "customWidth": "50",
      "name": "Threat Hunting Group",
      "styleSettings": {
        "margin": "200",
        "padding": "200",
        "showBorder": true
      }
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "datatable (Count: long, severity: string) [0, \"Informational\", 0, \"Low\", 0, \"Medium\", 0, \"High\"]\n| union\n    (\n    jamfprotect_CL\n    | where input_eventType_s <> \"\"\n    | extend severity = case(input_match_severity_d == 0, \"Informational\", input_match_severity_d == 1, \"Low\", input_match_severity_d == 2, \"Medium\", input_match_severity_d == 3, \"High\", \"True\")\n    | where severity != \"True\"\n    | summarize Count = count() by severity\n    )\n| where isnotempty(severity)\n| summarize Count=sum(Count) by severity",
        "size": 3,
        "title": "All Alerts {_timetoken:value}",
        "timeContextFromParameter": "_timetoken",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "visualization": "tiles",
        "tileSettings": {
          "titleContent": {
            "columnMatch": "severity",
            "formatter": 18,
            "formatOptions": {
              "thresholdsOptions": "colors",
              "thresholdsGrid": [
                {
                  "operator": "==",
                  "thresholdValue": "Informational",
                  "representation": "green",
                  "text": "{0}{1}"
                },
                {
                  "operator": "==",
                  "thresholdValue": "Low",
                  "representation": "yellow",
                  "text": "{0}{1}"
                },
                {
                  "operator": "==",
                  "thresholdValue": "Medium",
                  "representation": "orange",
                  "text": "{0}{1}"
                },
                {
                  "operator": "==",
                  "thresholdValue": "High",
                  "representation": "redBright",
                  "text": "{0}{1}"
                },
                {
                  "operator": "Default",
                  "thresholdValue": null,
                  "representation": "green",
                  "text": "{0}{1}"
                }
              ],
              "compositeBarSettings": {
                "labelText": "",
                "columnSettings": []
              }
            }
          },
          "leftContent": {
            "columnMatch": "Count",
            "formatter": 1,
            "numberFormat": {
              "unit": 0,
              "options": {
                "style": "decimal",
                "useGrouping": true,
                "minimumSignificantDigits": 1,
                "maximumSignificantDigits": 3
              },
              "emptyValCustomText": "0"
            }
          },
          "showBorder": true,
          "sortOrderField": 1
        }
      },
      "name": "query - 14"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "jamfprotect_CL\n//| where input_host_hostname_s == '{_hostname:value}'\n| extend severity_ = tostring(parse_json(input_match_facts_s)[0].severity)\n| where isnotempty(severity_)\n| summarize count() by severity_, bin(TimeGenerated,{_timetoken:grain})\n| render areachart ",
        "size": 0,
        "title": "Events Detected (Count By Severity) {_timetoken:value}",
